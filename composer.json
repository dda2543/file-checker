{
    "name": "dda2543/file-cheker",
    "description": "Библиотека отслеживания изменения файлов в директории.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dda2543\\FileChecker\\": "src/",
            "Dda2543\\FileChecker\\Example\\": "example/"
        }
    },
    "authors": [
        {
            "name": "Дмитрий Демченко",
            "email": "dda2543@icloud.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "symfony/event-dispatcher": "^5.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "scripts": {
        "lint": " ./vendor/bin/php-cs-fixer fix --dry-run  --diff ./",
        "lint-fix": " ./vendor/bin/php-cs-fixer fix ./"
    }
}
